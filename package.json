{
  "name": "ts-proto-grpc-experiment",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "grpc-tools": "^1.9.1",
    "ts-proto": "^1.33.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "server-with-custom-package-def": "yarn build && node ./lib/server-with-custom-package-def.js",
    "server-with-proto-loader": "yarn build && node ./lib/server-with-proto-loader.js",
    "client-with-custom-package-def": "yarn build && node ./lib/client-with-custom-package-def.js",
    "build": "tsc -p .",
    "clean": "rm -rf lib",
    "proto": "grpc_tools_node_protoc --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./src/proto/greeter.proto --ts_proto_opt=outputClientImpl=false"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.1.5"
  }
}
